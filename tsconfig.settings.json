{
  "compilerOptions": {
    "target": "es2017",
    "declaration": true,
    "module": "commonjs",
    "outDir": "dist",
    "composite": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "declarationMap": true
  }
}
