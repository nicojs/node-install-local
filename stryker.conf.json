{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "testRunner": "mocha",
  "reporters": [
    "html",
    "progress",
    "dashboard"
  ],
  "checkers": [
    "typescript"
  ],
  "buildCommand": "npm run build",
  "mochaOptions": {
    "spec": [
      "dist/test/unit/**/*.js"
    ]
  },
  "coverageAnalysis": "perTest",
  "thresholds": {
    "high": 90,
    "low": 80,
    "break": 75
  }
}
